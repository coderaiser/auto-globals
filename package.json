{
  "name": "auto-globals",
  "version": "1.2.1",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "add browser globals on every test and remove when it is done",
  "homepage": "http://github.com/coderaiser/auto-globals",
  "main": "lib/auto-globals.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/auto-globals.git"
  },
  "scripts": {
    "test": "tape 'test/*.js'",
    "watch:test": "nodemon -w lib -w test -x \"npm test\"",
    "lint": "FORCE_COLOR=1 eslint lib test | eslint-remove-line",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@cloudcmd/stub": "^2.1.0",
    "currify": "^3.0.0"
  },
  "keywords": [
    "window",
    "tape",
    "global",
    "fetch"
  ],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.6.0",
    "eslint-remove-line": "^1.0.2",
    "nodemon": "^1.14.12",
    "nyc": "^13.0.1",
    "redrun": "^7.0.2",
    "tape": "^4.8.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.3.0"
  }
}
